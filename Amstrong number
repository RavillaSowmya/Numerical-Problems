int main()
{
    int i,n,temp,r,sum=0;
    temp=n;
    scanf("%d",&n);
    for(i=0;i<=n;i++)
       {
           r=n%10;
           sum=sum+(r*r*r);
           n=n/10;
       }
       if(temp==n)
           printf("it is a amstrong number");
       else
           printf("it is not a amstrong number");
    return 0;
}
